{
    "name": "techive/laravel-telebirr",
    "description": "A Laravel package for Telebirr payment gateway integration.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikiyas Aemero",
            "email": "jollyaemero2223.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Techive\\Telebirr\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Techive\\Telebirr\\TelebirrServiceProvider"
            ],
            "aliases": {
                "Telebirr": "Techive\\Telebirr\\Facades\\Telebirr"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}